{
  "name": "boilerplate-vite-react-ts-storybook-tailwind",
  "private": true,
  "version": "0.1.0",
  "description": "Boilerplate for Vite, React, TypeScript, Storybook, Tailwind",
  "author": "Alejandro Olivares",
  "license": "MIT",
  "homepage": "https://github.com/inmortalkaktus/boilerplate-vite-react-ts-storybook-tailwind#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/inmortalkaktus/boilerplate-vite-react-ts-storybook-tailwind.git"
  },
  "bugs": {
    "url": "https://github.com/inmortalkaktus/boilerplate-vite-react-ts-storybook-tailwind/issues"
  },
  "keywords": [
    "vite",
    "react",
    "typescript",
    "storybook",
    "tailwind"
  ],
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "lint": "eslint src/**/*.{js,jsx,ts,tsx,json}",
    "lint:fix": "eslint --fix src/**/*.{js,jsx,ts,tsx,json}",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx,css,md,json,scss} --config ./.prettierrc"
  },
  "dependencies": {
    "@vitejs/plugin-react": "^3.0.0",
    "autoprefixer": "^10.4.13",
    "postcss": "^8.4.20",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.3",
    "vite": "^4.0.0"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.0.0-beta.19",
    "@storybook/addon-interactions": "^7.0.0-beta.19",
    "@storybook/addon-links": "^7.0.0-beta.19",
    "@storybook/blocks": "^7.0.0-beta.19",
    "@storybook/react": "^7.0.0-beta.19",
    "@storybook/react-vite": "^7.0.0-beta.19",
    "@storybook/testing-library": "^0.0.13",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-react": "^7.31.11",
    "prettier": "2.8.1",
    "storybook": "^7.0.0-beta.19"
  }
